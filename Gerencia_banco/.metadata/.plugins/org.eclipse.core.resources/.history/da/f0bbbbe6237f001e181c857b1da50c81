package banco;

public class Cliente {
	private String nome;
	private String sobrenome;
	private String cpf;
	protected double saldo;
	
		Public gerenciaConta(String nome, String sobrenome, String cpf) {
		this.nome = nome;
		this.sobrenome = sobrenome;
		this.cpf = cpf;
		this.setSaldo(0.0);
	}

		public double getSaldo() {
			return saldo;
		}

		public void setSaldo(double saldo) {
			this.saldo = saldo;
		}
}
